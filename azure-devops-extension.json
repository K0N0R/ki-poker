{
  "manifestVersion": 1,
  "id": "planning-rooms-extension",
  "version": "0.1.0",
  "name": "Planning Rooms",
  "publisher": "k0n0r",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "A minimal extension to display planning rooms.",
  "categories": ["Azure Boards"],
  "contributions": [
    {
      "id": "planning-rooms-hub",
      "type": "ms.vss-web.hub",
      "targets": ["ms.vss-web.project-hub-group"],
      "properties": {
        "name": "Planning Rooms",
        "uri": "dist/index.html",
        "icon": {
          "default": "images/icon.png"
        }
      }
    }
  ]
}